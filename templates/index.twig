{% extends "base.twig" %}
{% block title %}Index{% endblock %}
{% block content %}

<section>
  <div class="container">
    <div class="entry-list">

      {% for entry in entries %}
        <article>
          <h2><a href="detail.twig?={{ entry['id'] }}">{{ entry['title'] }}</a></h2>
          <time datetime="2016-01-31 1:00">{{ entry['date'] }}</time>
        </article>
      {% endfor %}

    </div>
  </div>
</section>

{% endblock %}
